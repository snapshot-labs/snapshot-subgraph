{
  "name": "snapshot",
  "version": "0.1.1",
  "scripts": {
    "create": "graph create snapshot-labs/snapshot --node https://api.thegraph.com/deploy/",
    "create-local": "graph create snapshot-labs/snapshot --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy-studio": "graph deploy --studio snapshot subgraph.yaml",
    "deploy-studio-gnosis-chain": "graph deploy --studio snapshot-gnosis-chain subgraph.gnosis-chain.yaml",
    "deploy-studio-arbitrum-one": "graph deploy --studio snapshot-arbitrum-one subgraph.arbitrum-one.yaml",
    "deploy-studio-optimism": "graph deploy --studio snapshot-optimism subgraph.optimism.yaml",
    "deploy-studio-matic": "graph deploy --studio snapshot-matic subgraph.matic.yaml",
    "deploy-studio-fantom": "graph deploy --studio snapshot-fantom subgraph.fantom.yaml",
    "deploy-studio-sepolia": "graph deploy --studio snapshot-sepolia subgraph.sepolia.yaml",
    "deploy-studio-sonic": "graph deploy --studio snapshot-sonic subgraph.sonic.yaml",
    "deploy-studio-base": "graph deploy --studio snapshot-base subgraph.base.yaml",
    "deploy-studio-base-sepolia": "graph deploy --studio snapshot-base-sepolia subgraph.base-sepolia.yaml",
    "deploy-studio-blast-mainnet": "graph deploy --studio snapshot-blast-mainnet subgraph.blast-mainnet.yaml",
    "deploy-studio-linea": "graph deploy --studio snapshot-linea subgraph.linea.yaml",
    "deploy-studio-bsc": "graph deploy --studio snapshot-bsc subgraph.bsc.yaml",
    "deploy-mantle": "graph deploy snapshot-labs/snapshot-mantle subgraph.mantle.yaml --node https://subgraph-api.mantle.xyz/deploy --ipfs https://subgraph-api.mantle.xyz/ipfs",
    "deploy-local": "graph deploy snapshot-labs/snapshot --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.69.2",
    "@graphprotocol/graph-ts": "^0.26.0"
  }
}
